        ATL_USER_ID_COOKIE_NAME = "__aid_user_id",
        CROSS_DOMAIN_ID_COOKIE_NAME1 = "segment_cross_domain_id",
        CROSS_DOMAIN_ID_COOKIE_NAME2 = "seg_xid",
        CROSS_DOMAIN_ID_TS = 'seg_xid_fd',
        CROSS_DOMAIN_ID_FD = 'seg_xid_ts',
