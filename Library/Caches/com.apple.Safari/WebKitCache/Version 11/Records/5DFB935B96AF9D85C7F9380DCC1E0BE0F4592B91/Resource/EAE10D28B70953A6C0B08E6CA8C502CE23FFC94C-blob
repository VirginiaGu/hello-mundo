                <script>
    // native js logic for more menu button for better performance
    var clickElement = document.getElementsByClassName('header-link-more')[0],
        toggleElement = document.getElementsByClassName('header-nav-more')[0],
        mainMenuItem = document.querySelectorAll('.menu-main-item'),
        rightMenu = document.querySelectorAll('.js-header .header-nav-more .sub-menu')[0],
        header = document.getElementsByClassName('js-header')[0],
        overlayForMenu = document.getElementsByClassName('overlay-for-menu')[0],
        scrollTop = document.getElementById('js-scroll-top'),
        wrappers = document.querySelectorAll('html, body, #wrapper'),
        isMobile = false;

    if (/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {
        isMobile = true;
    }

    function runScroll() {
        scrollTo(document.body, 0, 600);
    }

    scrollTop.addEventListener("click", runScroll, false);

    function scrollTo(element, to, duration) {
        if (duration <= 0) return;
        var difference = to - element.scrollTop;
        var perTick = difference / duration * 10;

        setTimeout(function () {
            element.scrollTop = element.scrollTop + perTick;
            if (element.scrollTop === to) return;
            scrollTo(element, to, duration - 10);
        }, 10);
    }

    // open right menu
    clickElement.addEventListener('click', function () {
        this.parentNode.parentNode.classList.toggle('active');
        if (isMobile) {
            overlayForMenu.style.display = "block";

            // remove scroll while sidebar opened
            document.body.classList.toggle('sidebar-opened');

            //set right menu height: windowHeight - headerHeight
            rightMenu.style.height = window.innerHeight - header.offsetHeight + 'px';
        }
    }, false);


    // close right menu
    var closeMenu = function () {
        if (toggleElement.classList) {
            toggleElement.classList.remove('active');
        }
    };
    overlayForMenu.addEventListener('click', function () {
        closeMenu();
        // wait for menu slide animation
        overlayForMenu.style.display = "none";
        document.body.classList.remove('sidebar-opened');
    }, false);

    // close more menu dropdown by clicking on another menu item
            [].forEach.call(mainMenuItem, function (el) {
        el.addEventListener('click', function () {
            closeMenu();
        }, false);
    });

    // move scroll button from fixed header to body if tablet/mobile
    if (isMobile) {
        document.body.appendChild(scrollTop);
        document.body.appendChild(overlayForMenu);
    }
</script>

            </div>
        </div>
    </header>
    


