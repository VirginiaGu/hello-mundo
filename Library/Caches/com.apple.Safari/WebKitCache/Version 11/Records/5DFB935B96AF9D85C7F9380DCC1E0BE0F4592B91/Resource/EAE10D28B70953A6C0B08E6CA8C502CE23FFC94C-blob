                <script>
    //Main menu
    function handleClick(e) {
        var el = e.target;
        //disable first click for mobile devices, because of hover effect
        if (!el.isNotFirstClick) {
            el.isNotFirstClick = true;
            return;
        }
        //add class 'closed' for second click to close menu item
        if (el.isNotFirstClick && el.parentNode.classList) {
            el.parentNode.classList.toggle('closed');
        }
    }
    //reset isNotFirstClick and remove class 'closed' for case if another menu item clicked
    function handleMouseOver(e) {
        var el = e.target;
        el.isNotFirstClick = false;
        if (el.parentNode.classList) {
            el.parentNode.classList.remove('closed');
        }
    }
</script>

