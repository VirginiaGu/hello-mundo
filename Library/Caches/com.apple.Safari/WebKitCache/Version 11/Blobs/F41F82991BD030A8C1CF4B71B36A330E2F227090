function fixheigts(id) {
  /*---- Igualar Cajas en Noticias Relacionadas -----*/
  if (window.matchMedia('(min-width: 769px)').matches) {

    var elparent = jQuery(jQuery('.noticia-full')[id - 1]);

    var relatednewselemts = elparent.find('#block-views-news2-related > div > .view-content > div > div');

    var itmayaltos = [];
    relatednewselemts.each(function(i) {
      itmayaltos[i] = jQuery(this).height();
    });

    //la altura mas alta:
    var valmasalto = getMaxOfArray(itmayaltos);

    relatednewselemts.css('height', valmasalto);

    //-- ajustar titulo azul
    var itmayelemchil1 = elparent.find('#block-views-news2-related > div > .view-content > div > div:nth-child(1)');
    var altomaytit = itmayelemchil1.find('.view-content').height();
    itmayelemchil1.find('.view-header').css('height', (getMaxOfArray(itmayaltos) - (altomaytit + 30)));

    //ajustar el 3er bloque
    var htercerbloc = elparent.find('#block-views-news2-related > div > .view-content > div > div:nth-child(3)');

    var hblocka = htercerbloc.children('div').first();
    var hblockb = htercerbloc.children('div').last();

    hblockb.css({
      'height': valmasalto - (hblocka.height())
    });

  } //--final if min width
} //--fixheights

function menumobile() {
  /*---MENU responsive---*/
  if (window.matchMedia('(max-width: 768px)').matches) {
    jQuery('#wrapper').addClass('compact-head');
  } else {
    jQuery('#wrapper').removeClass('compact-head');
  }
}

function getMaxOfArray(numArray) {
  return Math.max.apply(null, numArray);
}

function insertflotevts() {
  var pevts = jQuery('.event-txt p');
  var ponlytext = pevts.filter(function() {
    //console.log(jQuery.trim(jQuery(this).text()).length);
    return jQuery.trim(jQuery(this).text()).length !== 0;
  });

  var numparrs = ponlytext.length;
  //console.log(numparrs);

  var eachp = parseInt(numparrs / 3);
  jQuery(ponlytext[eachp]).prepend('<div class="flotantes insertion evtdetall"></div>');
  jQuery(ponlytext[eachp]).children('.insertion').prepend(jQuery('.flotante-event'));

} //--insert evts

function insertflotantes(id) {

  //--traer el num act de la noticia
  var elparent = jQuery(jQuery('.noticia-full')[id - 1]);

  //---comprobar si hay flotantes
  var numflotants = elparent.find('.flotantes > div').length;

  if (numflotants > 0) {

    //---traer el contenido
    var elcontent = elparent.find('.content > div > *');

    //---filtrar el contenido por los parrafos con mÃ¡s de 100 palabras
    var ponlytext = elcontent.filter(function() {
      return jQuery.trim(jQuery(this).text()).length > 100;
    });

    //--cuantos parrafos hay?
    var numparrs = ponlytext.length;

    //--calcular cada cuantos parrafos insertar flotante
    var eachp = parseInt((numparrs / numflotants) / 3);

    //console.log('insertar cada: ' + eachp + '. Hay ' + numparrs + ' parrafos' + ' y ' + numflotants + ' flotantes');

    //----insertar flotantes en el texto
    for (var i = 0; i < numflotants; i++) {

      var place = parseInt(eachp + ((numparrs / numflotants) * i));

      jQuery(ponlytext[place]).prepend('<div class="flotantes insertion"></div>');
      jQuery(ponlytext[place]).children('.insertion').prepend(jQuery(elparent.find('.flotantes').children()[i]));
    }
  } //----if

} //--insertflotantes

function altoseventos() {

  if (window.matchMedia('(min-width: 768px)').matches) {
    var altoevent = [];
    jQuery('.view-id-eventos .item-list .views-row > article').each(function(i) {

      //--cada dos comparar y reescribir alto
      if (i % 2 === 0) {
        altoevent[0] = jQuery(this).outerHeight();
      } else {
        altoevent[1] = jQuery(this).outerHeight();

        jQuery(this).parent('.views-row').prev().children('article').css('height', getMaxOfArray(altoevent));
        jQuery(this).css('height', getMaxOfArray(altoevent));
      }
    });

    ///---next eventos en ficha detalle
    normalize_heights(jQuery('.next-events article'));
    /*jQuery('.next-events article').each(function(i) {
      //console.log(altoevent);
      //--cada dos comparar y reescribir alto
      if (i % 2 === 0) {
        altoevent[0] = jQuery(this).outerHeight();
      } else {
        altoevent[1] = jQuery(this).outerHeight();

        jQuery(this).prev().css('height', getMaxOfArray(altoevent));
        jQuery(this).css('height', getMaxOfArray(altoevent));
      }
    });*/
    ///---next eventos en ficha detalle
    jQuery('.subevents article').each(function(i) {
      //console.log(altoevent);
      //--cada dos comparar y reescribir alto
      if (i % 2 === 0) {
        altoevent[0] = jQuery(this).outerHeight();
      } else {
        altoevent[1] = jQuery(this).outerHeight();

        jQuery(this).prev().css('height', getMaxOfArray(altoevent));
        jQuery(this).css('height', getMaxOfArray(altoevent));
      }
    });
  } //---final match query

}

function normalize_heights($elm) {
  if (window.matchMedia('(min-width: 768px)').matches) {
    var losaltos = [];
    $elm.each(function() {
      losaltos.push(jQuery(this).outerHeight());
    });
    var elmasalto = getMaxOfArray(losaltos);
    $elm.css('height', elmasalto);
  }
}

function lock_scroll() {
  // lock scroll position, but retain settings for later
  var scrollPosition = [
    self.pageXOffset || document.documentElement.scrollLeft || document.body.scrollLeft,
    self.pageYOffset || document.documentElement.scrollTop || document.body.scrollTop
  ];
  var html = jQuery('html'); // it would make more sense to apply this to body, but IE7 won't have that
  html.data('scroll-position', scrollPosition);
  html.data('previous-overflow', html.css('overflow'));
  html.css('overflow', 'hidden');
  window.scrollTo(scrollPosition[0], scrollPosition[1]);
  jQuery('#container').ontouchmove = function(e) { e.preventDefault(); }

}

function unlock_scroll() {
  // un-lock scroll position
  var html = jQuery('html');
  var scrollPosition = html.data('scroll-position');
  html.attr('style', '');
  //html.css('overflow', html.data('previous-overflow'));
  if (scrollPosition !== undefined) {
    window.scrollTo(scrollPosition[0], scrollPosition[1]);
  }

  jQuery('#container').ontouchmove = function(e) {
    return true;
  }
}

///////////////////////////////////////////////
// stylingOnAjaxScroll!!!!!!!!!
///////////////////////////////////////////////

(function($, window, document) {
  var counter = 0;

  $(function() {

    $('.noticia-full article').imagesLoaded(function() {
      stylingOnAjaxScroll(0);
    });

  });

  $(document).ajaxComplete(function() {
    counter++;
    $('.noticia-full article').imagesLoaded(function() {
      stylingOnAjaxScroll(counter);
    });

  });

  function stylingOnAjaxScroll(index) {

    //-------CONFIG PARA DETECCION
    var grandparents = '';

    var parentClass = 'noticia-full article'; //-----CLASE CONTENEDOR
    var childClass = 'social-links'; //-----CLASE ELEMENTO

    //----------CLASES DE LOS ESTILOS

    var parentDetectedClass = 'parent-detected';
    var childDetectedClass = 'child-detected';

    /////////////////////////////////////////////

    var $parentelm = $(grandparents + '.' + parentClass);
    var $childelem = $(grandparents + '.' + childClass);

    $parentelm.removeClass('.' + parentDetectedClass);
    $childelem.removeClass('.' + childDetectedClass);

    var arrwaypoints = [];

    if (window.matchMedia('(min-width: 1024px)').matches) { //----LIMITAR EN RESPONSIVE

      if ($parentelm.length !== 0 && $childelem.length !== 0) {
        //---DETECT PARENT ELEMENT
        arrwaypoints[0] = new Waypoint({
          element: $parentelm.get(index),
          handler: function(direction) {

            if (direction === 'down') {
              $(this.element)
                .addClass(parentDetectedClass)
                .find('.' + childClass)
                .removeClass(childDetectedClass);

            }
            if (direction === 'up') {
              $(this.element)
                .removeClass(parentDetectedClass);
              $(this.element)
                .find('.' + childClass)
                .addClass(childDetectedClass);
            }
          },
          offset: '-' + ($($parentelm.get(index)).height() - ($($parentelm.get(index)).find('.' + childClass).height() + 200)) + 'px'
            //offset:'bottom-in-view'
        });

        //console.log($($parentelm.get(index)).height());

        //---FADEOUT CHILD ELEMENT
        arrwaypoints[1] = new Waypoint({
          element: $parentelm.get(index),
          handler: function(direction) {

            if (direction === 'down') {
              $(this.element).find('.' + childClass)
                .css('opacity', 0);
            }
            if (direction === 'up') {
              $(this.element).find('.' + childClass)
                .css('opacity', 1);
            }
          },
          offset: '-' + ($($parentelm.get(index)).height() - ($($parentelm.get(index)).find('.' + childClass).height() * 2.5)) + 'px'
        });

        //---DETECT CHILD ELEMENT
        arrwaypoints[2] = new Waypoint({
          element: $childelem.get(index),
          handler: function(direction) {
            if (direction === 'down') {
              $(this.element)
                .addClass(childDetectedClass);
            }
            if (direction === 'up') {
              $(this.element)
                .removeClass(childDetectedClass);
            }
          },
          offset: '160px'
        });
      } else {
        //console.log('Change parent & child classes for stylingOnAjaxScroll!!!');
      }

    } //---matchMedia

  } //----FINAL stylingOnAjaxScroll

  //-----checkbox del formulario newsletter
  //http://centauri/bbva/bbvaopen4u/es/suscripcion-newsletter

  var $checksuscri = $('.suscripcion_newsletter input[type="checkbox"]');

  $('.checkskin').on('click', function() {
    var $this = $(this);
    if (!$this.hasClass('checkfill')) {
      $this.addClass('checkfill');
      $checksuscri.prop('checked', true);
    } else {
      $this.removeClass('checkfill');
      $checksuscri.prop('checked', false);
    }

  });

}(window.jQuery, window, document));

////// -->FIN Carolina functions

// remove translate from home page
jQuery(window).load(function() {
  if (jQuery('body').hasClass('home')) {
    var elem = document.getElementById('l10n-client-data');
    if (elem !== null) {
      elem.parentNode.removeChild(elem);
    }

    var elem = document.getElementById('l10n-client');
    if (elem !== null) {
      elem.parentNode.removeChild(elem);
    }

  }
});

