  Drupal.behaviors.competitionTabs = {
    attach: function(context) {
      $('.tabs a').click(function(e) {
        Drupal.behaviors.switch_tabs($(this));
        return false;
      });
      $('.tab_bot a').click(function(e) {
        Drupal.behaviors.switch_tabs($(this));
