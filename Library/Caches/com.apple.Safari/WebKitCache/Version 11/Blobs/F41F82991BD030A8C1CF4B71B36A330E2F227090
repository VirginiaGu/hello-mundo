var countfullnoticias = 1;

///------MASONRY config
var elmgrid = jQuery('.grid').masonry({
  itemSelector: '.grid-item',
  percentPosition: true,
  columnWidth: '.grid-sizer'
});

jQuery(document).ajaxComplete(function() {

  countfullnoticias++;

  //-----------------------------------------------------------//
  /*--ajustar altos noticias relacionadas---*/
  jQuery(jQuery('.noticia-full')[countfullnoticias - 1]).imagesLoaded(function() {
    fixheigts(countfullnoticias);
  });

  //-----------------------------------------------------------//
  /*--EL MASONRY---*/
  elmgrid.imagesLoaded(function() {
    elmgrid.masonry();
  });

  var elemasonry = jQuery('.grid');
  var maselmts = elemasonry.children('.grid-item:not(.calc)').addClass('calc');

  elmgrid.append(maselmts).masonry('appended', maselmts);

  //-----------------------------------------------------------//
  /*---Insertar flotantes  ---*/
  insertflotantes(countfullnoticias);

  //------ajustar altos eventos
  altoseventos();

  //---- si esta el menulateral abierto, cerrarlo
  jQuery('.menu-toggle').removeClass('is-active');
  jQuery('body').removeClass('traslated');

});
jQuery(window).load(function() {
  //------ajustar altos eventos
  altoseventos();

  //-----------------------------------------------------------//
  /*--- ajustar altos noticia relacionadas ---*/
  fixheigts(1);
  //-----------------------------------------------------------//

  if (!window.matchMedia('(max-width: 768px)').matches) {
    /*---ARREGLAR IGUALAR EBOOK HOME---*/
    var ebook_home_altos = [];
    jQuery('#ebook_info .view-content > div').each(function() {
      ebook_home_altos.push(jQuery(this).height());
    });
    jQuery('#ebook_info .view-content > div').css('height', getMaxOfArray(ebook_home_altos) + 34);
  }
  //-----------------------------------------------------------//
  /*--- ajustar Altos destacados Actualidad ---*/
  resize_destac_actualidad();

  jQuery(window).resize(function() {
    resize_destac_actualidad();
  });
}); //---load

function resize_destac_actualidad() {
  var h_dest_actualidad = [];
  var destacados_actualidad = jQuery('.view-display-id-destacada.destacada').find('.row-one');

  if (!window.matchMedia('(max-width: 768px)').matches) {
    destacados_actualidad.each(function(i) {
      h_dest_actualidad[i] = jQuery(this).height();
    });

    destacados_actualidad.css('height', getMaxOfArray(h_dest_actualidad));

    var maxheight = getMaxOfArray(h_dest_actualidad);
    var moresize_img = h_dest_actualidad[0];
    if (maxheight > moresize_img) {
      var morepixels = maxheight - moresize_img;
      jQuery('.view-display-id-destacada.destacada').find('.row-one:first-child').find('.imagen').css('height', 300 + morepixels);
    }
  } else {
    destacados_actualidad.css('height', 'auto');
  }
}

jQuery(window).ready(function() {

  jQuery('.slider_events ul').bxSlider({
    minSlides: 2,
    maxSlides: 2,
    slideWidth: 500,
    slideMargin: 34,
    infiniteLoop: false,

  });

  //-----------------------------------------------------------//
  /*--- Igualar altos appMarket Home ---*/
  normalize_heights(jQuery('#apps .content_app'));

  //-----------------------------------------------------------//
  /*---Insertar flotantes  ---*/
  insertflotantes(1);
  insertflotevts();

  //-----------------------------------------------------------//
  /*---MASONRY  ---*/
  var elemasonry = jQuery('.grid');
  elemasonry.prepend('<div class="grid-sizer"></div>');
  elemasonry.find('.grid-item').addClass('calc');

  elmgrid.imagesLoaded(function() {
    elmgrid.masonry();
  });

  //elmgrid.masonry();

  //-----------------------------------------------------------//
  /*---MENU HAMBURGUER----*/
  jQuery('header #links-section').append('<div> <button class="menu-toggle c-hamburger c-hamburger--htx" aria-controls="primary-menu" aria-expanded="false"><span>toggle menu</span></button> <div id="menulateral"></div> </div>');

  jQuery('#menulateral').append(jQuery('#navigation').html());

  //-----------------------------------------------------------//
  /*------MENU Scroll-----------*/

  var m_lastScrollTop = 0;

  jQuery(window).scroll(function() {

    var m_getscroll = jQuery(window).scrollTop();

    var st = m_getscroll;
    if (st > m_lastScrollTop) { //--------------DOWN

      if (!jQuery('#wrapper').hasClass('compact-head')) {
        jQuery('#wrapper').addClass('compact-head');
        //---FIX SOCIAL ICONS IN NEWS
      }

      jQuery('#navigation li.expanded').find('ul').slideUp();
      jQuery('#navigation li.expanded').removeClass('opensub');

    } else if (m_getscroll < 2) { //--------------UP

      if (window.matchMedia('(min-width: 769px)').matches) {
        jQuery('#wrapper').removeClass('compact-head');
      }

    }
    m_lastScrollTop = st;

  }); //---end scroll

  //-----------------------------------------------------------//
  //-----DROPDOWN MENU
  jQuery('#navigation li.expanded > span.nolink').on('click', function(event) {
    event.stopPropagation();
    jQuery('#navigation li.expanded > span.nolink').next('ul').slideUp();
    jQuery(this).next('ul').slideToggle();

  }); //---final on click
  jQuery('#wrapper header').on('click', function() {
    jQuery('#navigation li.expanded > span.nolink').next('ul').slideUp();
  });

  jQuery('.menu-toggle').on('click', function(event) {

    jQuery(this).toggleClass('is-active');
    var elembody = jQuery('body');
    elembody.toggleClass('traslated');
    if (elembody.hasClass('traslated')) {
      //console.log('abierto!');
      lock_scroll();
    } else {
      //console.log('cerrado!');
      unlock_scroll();
    }
  });

  menumobile();

  //-----------------------------------------------------------//
  //-- AJUSTE ALTO NAV LATERAL MENU MOBILE
  jQuery('#menulateral nav').css('height', jQuery(window).height() - 30);

  //-----------------------------------------------------------//
  //---- BOTON MORE / MINUS TAGS MENU
  var showmax;
  if (window.matchMedia('(max-width: 1024px)').matches) {
    showmax = 3;
  } else {
    showmax = 5;
  }

  var numtags = 0;
  var tagselem = jQuery('.home #act_n1 .temas .view-content > div');

  tagselem.each(function(i) {
    var elthis = jQuery(this);
    numtags++;

    if (i === showmax) {
      elthis.addClass('firstlast')
        .after('<div id="moretags"><a href="#">more</a></div>');

    } else if (i > showmax) {
      elthis.addClass('hidetags');
    }
  });

  //-----envolver hidetags
  jQuery('.hidetags').wrapAll('<div class="wraphidetags"></div>');
  var wraphidetags = jQuery('.wraphidetags');

  var morebtnelem = jQuery('#moretags');
  morebtnelem.on('click', function(e) {
    e.preventDefault();
    var elthis = jQuery(this);

    if (jQuery(this).hasClass('minus')) {

      wraphidetags.hide('slow');

      elthis.removeClass('minus');
      elthis.insertAfter(jQuery('firstlast'));
      elthis.parent().parent().parent().removeClass('moreopen');

    } else {

      wraphidetags.show('slow');
      elthis.addClass('minus');
      elthis.insertAfter(tagselem.last().parent());
      elthis.parent().parent().parent().addClass('moreopen');

    }
  }); //----tags

  //-----------------------------------------------------------//
  /*---MENU IDIOMAS---*/

  var langelem = jQuery('#block-locale-language');
  langelem.find('h2').text(langelem.find('.active').find('a').text());
  langelem.find('h2').css('display', 'block');
  langelem.find('h2.title').on('click', function() {
    jQuery(this).next('ul').slideToggle();
    jQuery(this).toggleClass('langopen');
  });

  // Ajuste videos
  var elemanchor = jQuery('.view-taxonomy-api .view-display-id-destacada .imagen a');

  elemanchor.on('click', function(event) {

    event.preventDefault();
    /*--ocultar imagen*/
    jQuery(this).parent().css('display', 'none');

    /*---play video---*/
    var elmvideo = jQuery(this).parent().parent().children('.video').find('iframe');
    var msrc = elmvideo.attr('src');
    var rep_msrc = msrc.replace('autoplay=0', '');
    elmvideo.attr('src', rep_msrc + '&autoplay=1');

  });

  //-----------------------------------------------------------//
  /*---- RESIZE menu ----*/
  jQuery(window).resize(function() {
    menumobile();
    //---- si esta el menulateral abierto, cerrarlo
    jQuery('.menu-toggle').removeClass('is-active');
    jQuery('body').removeClass('traslated');
    unlock_scroll();
  });

  //-----------------------------------------------------------//
  /*---SLIDER SECCION NUESTRAS APIS---*/
  function create_slider_apis() {

    var $ulslider = jQuery('#slider_nuestras_apis');

    var w_slider_wrapper = $ulslider.width();
    var w_gutter = 20;
    var w_col = 255;

    if (!window.matchMedia('(max-width: 400px)').matches) {
      var num_cols_floor = Math.floor(w_slider_wrapper / (w_gutter + w_col));
      var total_w_cols = w_col * num_cols_floor;
      var total_w_gutter = w_gutter * (num_cols_floor - 1);
      var w_total_all = total_w_cols + total_w_gutter;
      var calc_w_col = ((w_slider_wrapper - w_total_all) / num_cols_floor) + w_col;
    } else {
      var calc_w_col = w_col;
      var num_cols_floor = 1;
    }

    $ulslider.bxSlider({
      slideWidth: calc_w_col,
      minSlides: 1,
      maxSlides: num_cols_floor,
      moveSlides: 1,
      slideMargin: w_gutter,
      auto: true,
      pause: 6000,
      infiniteLoop: true,
      onSliderLoad: function() {
          var altosli = [];
          $ulslider.find('li').each(function() {
            altosli.push(jQuery(this).height());
          });
          $ulslider.find('li').css('height', getMaxOfArray(altosli));

          $ulslider.css('opacity', '1');

        } //---onslider load
    });
  }
  create_slider_apis();

  //----------------//////////////////////////////////////////////////////////
  //  FIN CAROLINA SCRIPT
  //----------------//////////////////////////////////////////////////////////

  //contador tiempo "when" - ficha evento
  if (typeof start_data_event !== 'undefined') {
    jQuery('.contador').downCount({
      date: start_data_event
    });
  }

  //var wrapper_top = $(".progress .wrapper").offset().top;
  jQuery(window).scroll(function() {
    //var wrapper_height = $(".progress .wrapper").height();

    // Affixes Progress Bars
    var top = jQuery(this).scrollTop();
    /*if (top > wrapper_top - 10) {
        $(".progress .wrapper").addClass("affix");
    }
    else {
        $(".progress .wrapper").removeClass("affix");
    }*/

    // Calculate each progress section
    jQuery(".noticia-full").each(function(i) {
      //nsole.log(top)
      var this_top = jQuery(this).offset().top;
      var height = jQuery(this).height();
      var this_bottom = this_top + height;
      var percent = 0;

      // Scrolled within current section
      if (top >= this_top && top <= this_bottom) {
        percent = Math.trunc(((top - this_top) / (height)) * 100);

        if (percent >= 100) {
          percent = 100;
        } else {

          var urlNextNode = jQuery(this).find('a#next').attr('href');
          var img = '<div class="loaderwrap"><div id="noticia-full-loader">Cargando...</div></div>';

          next_div = jQuery(this).next().attr('class');
          if (urlNextNode && next_div != 'loaderwrap' && next_div != 'noticia-full') {
            jQuery(this).after(img);
            jQuery.get(urlNextNode, function(data) {
              var content = jQuery(".noticia-full", data).html();
              content = '<div class="noticia-full">' + content + "</div>";
              jQuery('div.loaderwrap').remove();
              //jQuery('div.loaderwrap').css('display','none');
              jQuery(".noticia-full:last").after(content);
            })
          }
        }
        var urlNode = jQuery(this).find('h1').attr('data-noticia-slug');
        var titleNode = jQuery(this).find('h1').html();
        //var urlNode=jQuery(this).find('a#next').attr('href');
        history.replaceState(urlNode, "", urlNode);
        document.title = titleNode;

        jQuery(".progress").css("display", "block");
        jQuery(".progress").css("width", percent + "%");

      } else if (top > this_bottom) {
        percent = 100;
        jQuery(".progress").css("display", "none");
        jQuery(".progress").css("width", percent + "%");
      }

      /*if(Math.trunc(jQuery(window).scrollTop() + jQuery(window).height() - this_top) == jQuery(this).height()) { // Scroll hasta el bottom de la pagina                
      } */

    });
  });

}); //---- FIN: READY

