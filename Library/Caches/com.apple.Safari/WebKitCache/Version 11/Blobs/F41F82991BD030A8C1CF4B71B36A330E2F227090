      Drupal.behaviors.switch_tabs($('.defaulttab'));
      Drupal.behaviors.switch_tabs_bot($('.defaulttab_bot'));

      $('.accordion').find('.accordion-toggle').click(function() {
        estilo = $(this).attr('class');
        if (estilo.indexOf("selected") > 0) {
          $(this).removeClass("selected");
          $(this).next().hide('1000');
        } else {
          //Seleccion
          $('.accordion-toggle').removeClass("selected");
          $(this).addClass("selected");
          //Expand or collapse this panel
          $(this).next().show('1000');
          //Hide the other panels
          $(".accordion-content").not($(this).next()).hide('1000');
        }
      });

      $('html').click(function(e) {
        if (e.target.className != 'fc-event-title' && e.target.className != 'fc-event-inner') {
          $('.tooltipevent').remove();
        }
      });
    }
  }

  Drupal.behaviors.switch_tabs = function(obj) {

    $('.tab-content').hide();
    $('.tabs a').removeClass("selected");
    var id = obj.attr("rel");

    $('#' + id).show();
    $('.tabs a[rel=' + id + ']').addClass("selected");
  }

  Drupal.behaviors.switch_tabs_bot = function(obj) {
    $('.oscuro .tab-content').hide();
    $('.tabs_bot a').removeClass("selected");
    var id = obj.attr("rel");

    $('#' + id).show();
    $('.tabs_bot a[rel=' + id + ']').addClass("selected");
  }

  Drupal.behaviors.igualar = {
    attach: function(context) {
      var resize = 0;
      var lastSize = 0;
      var count = 0;

      function equalHeight(group) {
        tallest = 0;
        group.each(function() {
          thisHeight = $(this).height();
          if (thisHeight > tallest) {
            tallest = thisHeight;
          }
        });
        group.height(tallest);
      }

      function equalHeightResize(group) {
        tallest = 0;
        group.each(function() {
          original = $(this).height();
          thisHeight = $(this).find("p").height();
          if (thisHeight > tallest) {
            tallest = thisHeight;
          }

          if (tallest != resize) {
            resize = tallest;
            if (count > 0) {
              if (lastSize < resize) {
                value = lastSize - resize;
                group.height(original - value);
              }

              if (lastSize > resize) {
                value = lastSize - resize;
                group.height(original - value);
              }
            }
            lastSize = resize;
            count++;
          }
        });
      }

      $(window).load(function() {
        equalHeight($(".igualar"));
      });

      $(window).resize(function() {
        equalHeightResize($(".igualar"));
      })
    }
  }

})(jQuery);

/* MASMEDIOS Funcion para la recarga del reCAPTCHA de google*/
(function($, Drupal, window) {
  "use strict";
  Drupal.behaviors.recaptcha = {
    attach: function(context) {
      $('.g-recaptcha', context).each(function() {
        if (typeof grecaptcha === 'undefined') {
          return;
        }
        if ($(this).hasClass('drupal-recaptcha-processed')) {
          grecaptcha.reset();
        } else {
          grecaptcha.render(this, $(this).data());
          $(this).addClass('drupal-recaptcha-processed');
        }
      });
    }
  }

  window.drupalRecaptchaOnload = function() {
    $('.g-recaptcha').each(function() {
      grecaptcha.render(this, $(this).data());
      $(this).addClass('drupal-recaptcha-processed');
    });
  }

})(jQuery, Drupal, window);

/* FIN MASMEDIOS Funcion para la recarga del reCAPTCHA de google*/

