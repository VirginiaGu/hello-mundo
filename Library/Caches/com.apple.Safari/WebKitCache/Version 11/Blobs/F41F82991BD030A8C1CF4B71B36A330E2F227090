/*---DETECT TAG IN BANNER'S ARTICLE----*/

(function($, window, document) {

  var news_counter = 0;

  $(function() {

    setclass_contag(news_counter);

  });
  $(document).ajaxComplete(function() {
    news_counter++;

    setclass_contag(news_counter);

  });

  function setclass_contag(num) {

    var zero_ficha = $($('.noticia-full')[num]).find('article');
    if (zero_ficha.find('.back_blue .tag_des .field-type-taxonomy-term-reference').length !== 0) {
      zero_ficha.addClass('contag');
    }
  }

}(window.jQuery, window, document));
