//-------------------------------------//
/*-----------BLOG API-----------*/
(function($, window, document) {
  var hayDestacada;
  $(function() {
    if (jQuery('.blog-apimarket').length !== 0) {

      $('.grid-sizer').remove();
      console.log($('.grid').length)
      //___ SI ES BLOG API:

      jQuery('body').addClass('isblog');
      $itemsBlog = jQuery('.views-row');

      $wrapDestacadas = jQuery('.view-header .view-display-id-destacada');

      hayDestacada = jQuery('.view-header .view-display-id-destacada').children().length !== 0 ? true : false;

      $itemsBlog.find('img').imagesLoaded(function() {

        //---STYLING BOXES EACH 2, 3, 3
        if (hayDestacada) {
          $wrapDestacadas.find('.view-content').children().each(function() {
            style_pattern(jQuery(this), 'dos', 2, $wrapDestacadas.find('.view-content').children());
          });
        }
        calcBlogItems($itemsBlog, hayDestacada);
      });

    }
  }); //---ready
  jQuery(document).ajaxComplete(function() {
    //----CALC BLOG API
    calcBlogItems(jQuery('.views-row'), hayDestacada);
  });

  function calcBlogItems($items, destacada) {

    $items.each(function(i) {
      var $this = jQuery(this);

      if (i >= (destacada ? 0 : 2)) {

        if (i % 3 === (destacada ? 0 : 2)) {
          if (i % 2 === 0) {

            style_pattern($this, 'tres-b', 3, $items);
          } else if (i >= $items.length - 2) {

            style_pattern($this, 'tres-b', 3, $items);

          } else {
            style_pattern($this, 'tres-a', 3, $items);
          }
        }

      } else if (i === 0) {
        !destacada && style_pattern($this, 'dos', 2, $items);
      }
    });
  }

  function style_pattern(elm, name, num, group) {

    var
      $elms_arr = [],
      h_arr = [],
      ind = elm.index(),
      isTresA = (name === 'tres-a' ? true : false);

    //---ADD CLASSES

    for (var i = 0; i < num; i++) {

      var $actelm = group.eq(ind + i);
      $elms_arr.push(group.eq(ind + i));

    }

    $elms_arr.forEach(function(d, i) {
      d.addClass(name + ' pos' + (i + 1) + ' calculated');
    });

    //---SET HEIGHTS

    for (var i = 0; i < num; i++) {
      var $actelm_h = group.eq(ind + i);
      h_arr.push(($actelm_h.length !== 0 ? $actelm_h.get(0).offsetHeight : 0));
    }

    var max_h = getMaxOfArray(h_arr);

    $elms_arr.forEach(function(d, i) {
      if (isTresA) {

        if (i === 0) {
          var twoH = h_arr[1] + h_arr[2];

          d.height(twoH)
            .find('.field-type-image')
            .height(twoH - (h_arr[i] - 280));

        } else {
          d.height(h_arr[i]);
        }

      } else {
        d.height(max_h);
      }

    });

  }

}(window.jQuery, window, document));
;
