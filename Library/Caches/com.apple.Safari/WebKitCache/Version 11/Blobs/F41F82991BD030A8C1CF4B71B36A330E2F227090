jQuery(document).ready(function() {

  if (jQuery('.noticia-full').length !== 0) {
    jQuery(window).load(function() {
      activate_modulo(0, (jQuery(window).height() / 1.2));
    }); //---LOAD
  } else {
    activate_modulo(0, (jQuery(window).height() / 2));
  }

}); //---READY

var modulos_counter = 0;
jQuery(document).ajaxComplete(function() {
  jQuery('#conatiner').imagesLoaded(function() {
    
    activate_modulo(modulos_counter, (jQuery(window).height() / 1.8));
  });

});

function activate_modulo(sum, pos) {
  var modulos_act = jQuery('.dato-modulo-actualidad');

  if (modulos_act.length > 0) {

    var activated = {};

    modulos_act.each(function(i) {

      console.log('modulos_counter: ' + modulos_counter);
      if (i >= sum) {
        var elm = jQuery(this);
        var elm_offset = elm.offset();

        activated['elm' + (i + sum)] = false;

        jQuery(window).scroll(function() {

          if ((jQuery(window).scrollTop() + pos) > elm_offset.top) {

            if (!activated['elm' + (i + sum)]) {
              eachcirclecreator(elm.get(0), (i + sum));
              jQuery(elm.get(0)).addClass('init');
              activated['elm' + (i + sum)] = true;
            }
          }
        }); //---FIN SCROLL

        modulos_counter++;

      }
    }); //---fin:each
    //console.log(JSON.stringify(activated));
  }
}

function eachcirclecreator(elm, num) {

  var actcircle = elm;
  var val = parseInt(actcircle.textContent);

  var w_circles = 140;
  var w_stroke = 12;
  var svgbox = {
    'w': w_circles + w_stroke,
    'h': w_circles + w_stroke
  };

  //gradient

  var objcfgcircle = {
    'offorig': {
      x: svgbox.w / 2,
      y: svgbox.h / 2
    },
    'arr': [100.00001 - val, val],
    'color': ['white', 'transparent'],
    'grades': 360,
    'radio': w_circles / 2,
    'rotation': 270 + 90,
    'strokew': w_stroke,
    'strokedata': false

  };
  var data = objcfgcircle.arr;
  var color = objcfgcircle.color;
  var svgns = "http://www.w3.org/2000/svg";
  var xlinkns = "http://www.w3.org/1999/xlink";

  var svg = document.createElementNS(svgns, 'svg');
  svg.setAttribute('width', svgbox.w);
  svg.setAttribute('height', svgbox.h);
  svg.setAttribute('id', 'svgwrap' + num);
  svg.setAttribute('class', 'circle-svg-wrap');

  actcircle.appendChild(svg);
  var circlebase = document.createElementNS(svgns, 'circle');

  //---add defs
  var defs = document.createElementNS('http://www.w3.org/2000/svg',
    'defs');
  svg.appendChild(defs);
  var gradient = document.createElementNS('http://www.w3.org/2000/svg', 'linearGradient');
  gradient.setAttribute('id', 'linear');
  gradient.setAttribute('x1', '0%');
  gradient.setAttribute('y1', '0%');
  gradient.setAttribute('x2', '0%');
  gradient.setAttribute('y2', '100%');
  defs.appendChild(gradient);

  var stop1 = document.createElementNS('http://www.w3.org/2000/svg', 'stop');
  var stop2 = document.createElementNS('http://www.w3.org/2000/svg', 'stop');
  stop1.setAttribute('offset', '0%');
  stop1.setAttribute('stop-color', '#e3e3e3');
  stop2.setAttribute('offset', '100%');
  stop2.setAttribute('stop-color', '#9fd750');

  gradient.appendChild(stop1);
  gradient.appendChild(stop2);

  //----CIRCLES
  circlebase.setAttribute('cx', objcfgcircle.offorig.x);
  circlebase.setAttribute('cy', objcfgcircle.offorig.y);
  circlebase.setAttribute('r', objcfgcircle.radio);
  circlebase.setAttribute('stroke-width', objcfgcircle.strokew);
  circlebase.setAttribute('stroke', "url(#linear)");
  circlebase.setAttribute('stroke-opacity', 0.5);
  circlebase.setAttribute('fill', 'none');
  svg.appendChild(circlebase);

  //--DRAW data
  setTimeout(function() {

    for (var itemdata in data) {
      drawarcs(parseInt(itemdata), objcfgcircle, data[parseInt(itemdata)], 'svgwrap' + num, color);
    }
  }, 200);
} //----fin: eachcirclecreator
function drawarcs(i, objcfg, d, wrap, color) {

  var raddata = circlecoords(objcfg);

  var angles = raddata[2];
  var svgns = "http://www.w3.org/2000/svg";
  var aPath3 = document.createElementNS(svgns, 'path');
  document.getElementById(wrap).appendChild(aPath3);

  aPath3.setAttribute('d',
    ' M' + (raddata[0][i + 1].rx) + ' ' + (raddata[0][i + 1].ry) +
    ' A' + '-' + (objcfg.radio) + ' ' + (objcfg.radio) + ' 0 ' + (angles[i] < 180 ? 0 : 1) + ' 1 ' +
    (raddata[0][i].rx) + ' ' + (raddata[0][i].ry)
  );

  var elcolor = color[0];

  if (i === 0) {
    var easing = 'easeInCubic';
    var duration = 600;
    var delay = 600;
    elcolor = color[1];
  } else {
    var easing = 'easeOutBounce';
    var duration = 1200;
    var delay = 0;
  }

  aPath3.setAttribute('stroke', elcolor);
  aPath3.setAttribute('stroke-width', objcfg.strokew);

  aPath3.setAttribute('fill', 'none');
  aPath3.setAttribute('id', wrap + '_path' + i);

  //-----ANIMATE

  var length = aPath3.getTotalLength();
  aPath3.getBoundingClientRect();

  jQuery(aPath3).css({
    'stroke-dasharray': length + ' ' + length,
    'stroke-dashoffset': length
  });

  jQuery(aPath3).delay(delay).animate({
    'stroke-dashoffset': '0'
  }, duration, easing, function() {});
} //---drawarcs

function circlecoords(objcfg, off) {

  var off = typeof off !== 'undefined' ? off : 0;

  ///---fuera
  var offorig = objcfg.offorig;
  var arr = objcfg.arr;
  var grades = objcfg.grades;
  var radio = objcfg.radio + off;
  var rotation = objcfg.rotation;

  ///--locals
  var coordscircle = [];

  var getsum = misumelemarr(arr, grades);

  var sum = getsum[0];
  var onlyangles = getsum[1];
  var sumcenter = [];

  //for (var i = sum.length - 1; i >= 0; i--) {
  for (var i = 0; i < sum.length; i++) {
    sum[i] = sum[i] + rotation;

    var rx = (Math.sin(Math.radians(sum[i])) * radio) + offorig.x;
    var ry = (Math.cos(Math.radians(sum[i])) * radio) + offorig.y;

    var lasum;
    if (i + 1 < sum.length) {
      lasum = sum[i + 1] + rotation;
    } else {
      lasum = sum[0];
    }
    sumcenter.push(sum[i] + ((lasum - sum[i]) / 2));
    var add = 10;
    var cx = (Math.sin(Math.radians(sum[i] + ((lasum - sum[i]) / 2))) * radio) + offorig.x;
    var cy = (Math.cos(Math.radians(sum[i] + ((lasum - sum[i]) / 2))) * radio) + offorig.y;

    coordscircle.push({ rx: rx, ry: ry, cx: cx, cy: cy });

  } //--- for

  return [coordscircle, sum, onlyangles, sumcenter];
} //------final coords

///////////////////////////
/// Auxiliary functions ///
///////////////////////////

function normalizevals(arr, grades) {

  var total = 0;
  var totalnorm = 0;
  var localarr = [];

  for (var i = 0; i < arr.length; i++) {
    total += arr[i];
  }

  for (i = 0; i < arr.length; i++) {
    localarr.push(((arr[i]) / total) * (grades));
  }
  return localarr;
}

function misumelemarr(arr, grades) {
  var losangles = normalizevals(arr, grades);
  var arrsum = [0];
  var total = 0;

  for (var i = 0; i < losangles.length; i++) {
    total += losangles[i];

    if (i !== 0) {
      arrsum[i + 1] = total;
    } else {
      arrsum[i + 1] = losangles[i];
    }
  }
  return [arrsum, losangles];
}

Math.radians = function(degrees) {
  return degrees * Math.PI / 180;
};
