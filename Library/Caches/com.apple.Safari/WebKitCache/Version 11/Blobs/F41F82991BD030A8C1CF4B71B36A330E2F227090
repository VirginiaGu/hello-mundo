/*------GALERIA FOTOS FICHA-----------*/
(function($, window, document) {
  var news_counter = 0;
  $(function() {
    //gallery_ficha(news_counter);

    masonry_inside_ficha(news_counter);

  });

  $(document).ajaxComplete(function() {
    news_counter++;
    //gallery_ficha(news_counter); 
    masonry_inside_ficha(news_counter);

  });

  function gallery_ficha(num) {
    var noticia_act = $($('.noticia-full').get(num));
    var galeria_ficha = noticia_act.find('.gallery .field-items');
    if (galeria_ficha !== 0) {
      galeria_ficha.find('.field-item').each(function() {
        $(this).append('<div class="gallery-caption">' + $(this).find('img').attr('title') + '</div>');
      });
      galeria_ficha.bxSlider({
        auto: true,
        pause: 5000
      });
    }
  }

  function masonry_inside_ficha(num) {
    var noticia_act = $($('.noticia-full').get(num));
    var galeria_ficha = noticia_act.find('.gallery .field-items');
    galeria_ficha.addClass('gallery-masonry-news');
    galeria_ficha.prepend('<div class="field-sizer"></div>');

    /*
    ///---LINK PARA PONER EN BLOQUE DE LA NOTICIA
    <a href="http://centauri/bbva/bbvaopen4u/es/actualidad/necesitas-un-prestamo-tu-unica-posibilidad-es-convencer-un-algoritmo" class="link-galeria-masonry" style="display:none">Ver galería completa</a>
    */

    var link_galeria_masonry = noticia_act.find('.link-galeria-masonry');
    var link_ver_mas = link_galeria_masonry.attr('href') !== undefined ? link_galeria_masonry.attr('href') : 'none';

    if (galeria_ficha !== 0) {
      galeria_ficha.find('.field-item').each(function() {

        var elthis = $(this);
        var laimg = elthis.find('img');
        var texto_caption = elthis.find('img').attr('title') !== undefined ? elthis.find('img').attr('title') : 'none';

        laimg.wrap('<a class="wrap-anchor" href="' + laimg.attr('src') + '"></a>');

        if (texto_caption !== 'none') {
          elthis.find('.wrap-anchor').append('<div class="gallery-caption">' +
            '<div class="txt">' + texto_caption + '</div>' + '</div>');
        }
      }); //----fin: each

      if (link_ver_mas !== 'none') {
        link_galeria_masonry.css('display', 'inline-block');
        galeria_ficha.parent().append(link_galeria_masonry);
        link_galeria_masonry.wrap('<div class="wrap-link-galeria"></div>');
      }

      galeria_ficha.css('display', 'block');
      $('gallery-caption').css('display', 'block');

      noticia_act.imagesLoaded(function() {

        var $grid = galeria_ficha.masonry({
          itemSelector: '.field-item',
          percentPosition: true,
          columnWidth: '.field-sizer'
        });
      });

      galeria_ficha.magnificPopup({
        delegate: 'a',
        type: 'image',
        mainClass: 'mfp-fade',
        gallery: {
          enabled: true,
          navigateByImgClick: true,
          preload: [0, 1] // Will preload 0 - before current, and 1 after the current image
        },
        image: {
          titleSrc: function(item) {
            console.log()
            return item.el.find('img').attr('title');
          }
        }
      });

    } //---fin ficha exists   
  }

}(window.jQuery, window, document));

