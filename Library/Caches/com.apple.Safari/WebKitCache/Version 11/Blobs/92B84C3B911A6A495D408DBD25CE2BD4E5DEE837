t}\n\n\treturn navHTML.join('\\n');\n}\n\nif (curProject === 'mscom') {\n\t// UPDATE UHF NAV FOR MSCOM \n\t$('#uhf-g-nav').remove();\n\t$('.js-global-head').after(['<div class=\"theme-light  js-cat-head\" itemprop=\"brand\" itemscope=\"itemscope\" itemtype=\"http://schema.org/Brand\" ms.cmpgrp=\"nav\" data-grid=\"container\">',\n\t\t'<div>',\n\t\t'<a class=\"c-logo\" href=\"https://www.microsoft.com/\" aria-label=\"Microsoft\" itemprop=\"url\" ms.title=\"Microsoft\">',\n\t\t'<img itemprop=\"logo\" itemscope=\"itemscope\" class=\"c-image\" alt=\"Microsoft\" src=\"https://assets.onestore.ms/cdnfiles/external/uhf/long/9a49a7e9d8e881327e81b9eb43dabc01de70a9bb/images/microsoft-gray.png\">',\n\t\t'<span>Microsoft</span>',\n\t\t'</a>',\n\t\t'<button class=\"c-action-trigger c-glyph glyph-chevron-left\" ms.title=\"Previous\" tabindex=\"-1\" style=\"display: none;\"></button>',\n\t\t'<button class=\"c-action-trigger c-glyph glyph-chevron-right\" ms.title=\"Next\" tabindex=\"-1\" style=\"display: block;\"></button>',\n\t\t'<nav id=\"uhf-c-nav\" role=\"navigation\" class=\"exp-pmg\" ms.cmpgrp=\"nav\" style=\"display: block;\">',\n\t\tcreateNavStructure(),\n\t\t'</nav>',\n\t\t'</div>',\n\t\t'</div>'\n\t].join('\\n'));\n\n\t$('#uhfLogo').hide();\n\t$('.js-cat-head:eq(1)').addClass('exp-pmg-cat');\n\t$('.js-cat-head:eq(1) nav').addClass('exp-pmg-cat-nav');\n\t$('.exp-pmg-cat').css('z-index','500');\n\n\t//cat nav slide initialize\n\t$('#mobileHomeLink').css({'margin-left':'0px','transition':'margin 0.667s cubic-bezier(0.16, 1, 0.29, 0.99) 0s'});\n\n\t//mobile & cat nav initialize\n\tvar mobileFix = setTimeout(function(){\n\t\tvar current_width = document.documentElement.clientWidth;\n\t\t//mobile fix\n\t\tif(current_width <= 767){\n\t\t\t$('.js-cat-head:eq(0) nav').attr('id','uhf-g-nav');\n\t\t}\n\t\t//cat nav fix\n\t\tif((767 < current_width)&&(current_width < 777)){\n\t\t\t$('.js-cat-head:eq(1) button.glyph-chevron-right').show();\n\t\t}\n\t},100);\n\n\t//slider functions\n\t$(