"use strict";

document.addEventListener("DOMContentLoaded", function() {

	var menuButton = document.getElementById("menu-button");
	if (menuButton) {
		menuButton.addEventListener("click", toggleMenu);
		menuButton.addEventListener("tap", toggleMenu);
	}
	function toggleMenu() {
		var burger = document.querySelector(".burger");
		burger.toggleClass("visible");
	};

	Element.prototype.addClass = function (className) {
		if (this.classList) {
			this.classList.add(className);
		} else {
			this.className += " " + className;
		}
	}

	Element.prototype.removeClass = function (className) {
		if (this.classList) {
			this.classList.remove(className);
		} else {
			this.className = this.className.replace(new RegExp('(^|\\b)' + className.split(' ').join('|') + '(\\b|$)', 'gi'), ' ');
		}
	}

	Element.prototype.toggleClass = function (className) {
		if (this.classList) {
			this.classList.toggle(className);
		} else {
			var classes = this.className.split(" ");
			var existingIndex = classes.indexOf(className);
			if (existingIndex >= 0) {
				classes.splice(existingIndex, 1);
			} else {
				classes.push(className);
			}
			this.className = classes.join(" ");
		}
	}

});
document.addEventListener("DOMContentLoaded", function() {

	var downloadButton = document.getElementById("download-button");
	if (downloadButton) {
		if (/iPad|iPhone|iPod/i.test(navigator.userAgent)) {
			downloadButton.textContent = "Download for iOS";
			downloadButton.href = "https://itunes.apple.com/us/app/1password-password-manager/id568903335?mt=8&uo=4&at=10l4Hf&pt=11798&ct=site";
			downloadButton.setAttribute("target", "_blank");
		} else if (/Mac/i.test(navigator.userAgent)) {
			// Do nothing, leave "Try it FREE" button.
		} else if (/Android/i.test(navigator.userAgent)) {
			downloadButton.textContent = "Download for Android";
			downloadButton.href = "https://play.google.com/store/apps/details?id=com.agilebits.onepassword";
			downloadButton.setAttribute("target", "_blank");
		} else if (/Windows/i.test(navigator.userAgent)) {
			// Do nothing, leave "Try it FREE" button.
		}
	}

	var playVideoButton = document.getElementById("play-video");
	if (playVideoButton) {
		playVideoButton.addEventListener("click", playVideo);
		playVideoButton.addEventListener("tap", playVideo);
	}

	function playVideo() {
		document.querySelector(".video-container .video").innerHTML = '<iframe width="853" height="480" src="https://www.youtube-nocookie.com/embed/mcly2-b1W20?rel=0&amp;controls=0&amp;showinfo=0;autoplay=1" frameborder="0" allowfullscreen></iframe>';
		document.querySelector(".video-container").toggleClass("visible");
		document.querySelector("#play-video").toggleClass("hidden");
		document.querySelector(".home-intro").toggleClass("hidden");
	};

	var stopVideoButton = document.getElementById("video-close");
	if (stopVideoButton) {
		stopVideoButton.addEventListener("click", stopVideo);
		stopVideoButton.addEventListener("tap", stopVideo);
	}

	function stopVideo() {
		document.querySelector(".video-container").toggleClass("visible");
		document.querySelector(".video-container .video").innerHTML = '';
	};

	if (document.getElementById('popup')) {
		window.onscroll = function() {
		  var navscroll = document.getElementById('popup');
		  if (document.getElementById('close-popup')) {
			var dismiss = document.getElementById('close-popup');
		  }
		  if ( window.pageYOffset > 10 ) {
		    navscroll.classList.remove("hidden");
		  } else if ( window.pageYOffset < 10 ) {
		    navscroll.classList.add ("hidden");
		  }
		  dismiss.addEventListener('click', function(e) {
			navscroll.parentElement.removeChild(navscroll);
		  });
		}
	}
});
document.addEventListener("DOMContentLoaded", function() {

	var cardSelectors = document.querySelectorAll("section.cards .teams p");
	[].forEach.call(cardSelectors, function(e) {
		e.addEventListener("mouseenter", switchCard);
		e.addEventListener("click", switchCard);
		e.addEventListener("tap", switchCard);
	});
	function switchCard() {
		var selectedCard = document.querySelector("section.cards .teams p.selected");
		var cards = document.querySelector("section.cards .card");
		cards.className = "";
		selectedCard.removeClass("selected");
		cards.addClass("card");
		cards.addClass(this.className);
		this.addClass("selected");
	};
	var header = document.querySelector("section.press-sidebar");
	var hero = document.querySelector(".press");
	if (hero) {
		var distance = hero.clientHeight;
		window.onscroll = function() {
			if (window.pageYOffset >= distance) {
				header.addClass("fixed");
			} else {
				header.removeClass("fixed");
			}
		};
		window.onload = function() {
			distance = hero.clientHeight;
		};
		window.onresize = function() {
			distance = hero.clientHeight;
		};
	}
});document.addEventListener("DOMContentLoaded", function() {

	var couponId = getURLParameter(window.location.href, "C");
	if (couponId) {
		try {
			var date = new Date();
			date.setHours(date.getHours() + 48);
			localStorage.setItem("couponId", couponId);
			localStorage.setItem("couponExpiry", date.valueOf());
		} catch (e) {
			console.warn("Unable to save coupon in local storage.");
		}
	} else {
		try {
			couponId = localStorage.getItem("couponId");
			if (couponId) {
				var couponExpiry = localStorage.getItem("couponExpiry");
				if (!couponExpiry || new Date().valueOf() > Number(couponExpiry)) {
					couponId = "";
					localStorage.removeItem("couponId");
					localStorage.removeItem("couponExpiry");
				}
			}
		} catch (e) {
			console.warn("Unable to check for coupon in local storage.");
		}
	}

	var signUpElements = document.querySelectorAll("[data-signup-action]");
	if (couponId && signUpElements) {
		for (var i = 0; i < signUpElements.length; i++) {
			var signUpElement = signUpElements[i];
			if (signUpElement.href) {
				if (signUpElement.href.indexOf("?c=") !== -1 || signUpElement.href.indexOf("&c") !== -1) {
					continue;
				}

				var separator = signUpElement.href.indexOf("?") > -1 ? "&" : "?";
				signUpElement.href = signUpElement.href + separator + "c=" + encodeURIComponent(couponId);
			} else if (signUpElement.action) {
				var input = document.createElement("input");
				input.setAttribute("type", "hidden");
				input.setAttribute("name", "c");
				input.setAttribute("value", couponId);
				signUpElement.appendChild(input);
			}
		}
	}

	function getURLParameter(url, key) {
		var match = url.toUpperCase().match('[?&]' + key + '=([^&]+)');
		return match ? match[1] : null;
	}

});
document.addEventListener("DOMContentLoaded", function() {

	var legacyLink = document.querySelector(".legacy-link");
	if (legacyLink) {
		legacyLink.addEventListener("click", toggleLegacyDownloadsSection);
	}

	function toggleLegacyDownloadsSection() {
		var legacyDownloadsSection = document.querySelector(".legacy-downloads-container");
		legacyDownloadsSection.toggleClass("show");
		legacyDownloadsSection.toggleClass("hide");
	}

	var betaToggle = document.querySelector(".beta-toggle");
	if (betaToggle) {
		betaToggle.addEventListener("click", toggleBetas);
	}

	function toggleBetas() {
		var betaDownloads = document.querySelectorAll(".beta");
			[].forEach.call(betaDownloads, function(e) {
				e.toggleClass("show");
				e.toggleClass("hide");
			});
		var betaDownloads = document.querySelectorAll(".non-beta");
			[].forEach.call(betaDownloads, function(f) {
				f.toggleClass("show");
				f.toggleClass("hide");
		});
		var betaToggleChange = document.querySelector(".beta-toggle");

			if (betaToggleChange.textContent === "Enable betas") {
				betaToggleChange.textContent = "Disable betas";
			}

			else {
				betaToggleChange.textContent = "Enable betas";
			}
	}
});
document.addEventListener("DOMContentLoaded", function() {

	var licenses = document.querySelector("#licenses a");
	if (licenses) {
		licenses.addEventListener("click", toggleLicensesLearnMore);
	}

	function toggleLicensesLearnMore(e) {
		var learnMoreLink = document.querySelector("#licenses a");
		learnMoreLink.remove();
		var learnMore = document.querySelector("#licenses-learn-more");
		learnMore.toggleClass("hide");
	}
});
