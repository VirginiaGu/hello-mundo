utils.waitForElement('#homepage--hero--video').then(function(el){
  var $this = $(el);
  var $cta = $('<a href="'+extension.ctaLink+'" class="'+extension.ctaClasses+'">'+extension.ctaLabel+'</a>');
  if (extension.ctaTarget === 'new') $cta.attr('target', '_blank');

  $cta.bind('mousedown touch', function(){
    window.optimizely = window.optimizely || [];
    window.optimizely.push({
      type: "event",
      eventName: "homepage_hero_click"
    });
  });

  var $left = $this.find('.hero-text.text-left'), $right = $left.next();
  if (extension.columns === '2+3'){
    $left.removeClass('colspan-3').addClass('colspan-2');
    $right.removeClass('colspan-2').addClass('colspan-3');
  }
  if (extension.columns === '3+2'){
    $left.removeClass('colspan-2').addClass('colspan-3');
    $right.removeClass('colspan-3').addClass('colspan-2');
  }

  $left.find('.block--heading-text-block > h1').text(extension.header);
  $left.find('.block--heading-text-block > p').text(extension.subheader);
  $left.find('.video-link, .cta').replaceWith($('<div class="cta">').append($cta));
  $right.find('.hero-illustration').attr('src', extension.img);
});
