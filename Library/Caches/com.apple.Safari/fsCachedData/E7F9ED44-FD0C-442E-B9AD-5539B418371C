    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js" type="text/javascript"></script>
        <script>
            (function() {
                var main = null;

                var main = function(defaultDisplayLocation, cookiePrefix) {
    var cookieName = cookiePrefix + 'location';

    var readCookie = function(name) {
        var nameEQ = name + "=";
        var ca = document.cookie.split(';');
        for(var i=0;i < ca.length;i++) {
            var c = ca[i];
            while (c.charAt(0)==' ') c = c.substring(1,c.length);
            if (c.indexOf(nameEQ) === 0) {
                return c.substring(nameEQ.length,c.length);
            }
        }
        return null;
    };

    var unquotePlus = function(s) {
        return decodeURIComponent(s.replace(/\+/g, ' '));
    };

    $(function() {
        var locationCookie = readCookie(cookieName);
        if (locationCookie) {
            var userCookieLocMaybeJson = unquotePlus(locationCookie);
            var displayLocation = defaultDisplayLocation;
            if (userCookieLocMaybeJson && (userCookieLocMaybeJson != 'null')){
                try {
                    var jsonLocation = jQuery.parseJSON(userCookieLocMaybeJson);
                    displayLocation = jsonLocation.unformatted;
                } catch(e) {
                    displayLocation = userCookieLocMaybeJson;
                }
            }
            $('#find-loc').val(displayLocation);
        }
    });
};


                if (main === null) {
                    throw 'invalid inline script, missing main declaration.';
                }
                main("San Francisco, Kalifornien","dev-");
            })();
    </script>
